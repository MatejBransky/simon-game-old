{
  "name": "simon-game",
  "version": "1.0.0",
  "repository": "git@github.com:MatejBransky/simon-game.git",
  "author": "Matej Bransky <bransky@logio.cz>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest --watch",
    "test:e2e": "cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --ext ts --ext tsx --ext js --ext jsx",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@types/jest": "^24.0.19",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "cypress": "^3.4.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.0-0",
    "parcel": "^1.12.4",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  }
}
